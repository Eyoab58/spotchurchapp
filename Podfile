platform :ios, '14.0'

target 'spotchurchapp' do
  use_frameworks!

  # Firebase pods
  pod 'FirebaseCore', '~> 9.6'
  pod 'FirebaseAuth', '~> 9.6'
  pod 'FirebaseFirestore', '~> 9.6'
  pod 'FirebaseStorage', '~> 9.6'
  pod 'FirebaseAnalytics', '~> 9.6'
  pod 'FirebaseMessaging', '~> 9.6'

  target 'spotchurchappTests' do
    inherit! :search_paths
  end

  target 'spotchurchappUITests' do
    inherit! :search_paths
  end
end
